<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog id="selectDialogg"
      multiSelect="true"
       title="{i18n>dlgTitleSelectAssetNumber}"  
       items="{
            path: '/AssetDetailVH',
            sorter: {
                path: 'FixedAssetDescription',
                descending: false
            }
        }" 
        search=".onGenericValueHelpSearch" 
        confirm=".onGenericValueHelpClose" 
        cancel=".onGenericValueHelpClose">
    <customData>
        <core:CustomData key="filterFields" value="MasterFixedAsset,FixedAssetDescription" />
    </customData>
    <StandardListItem title="{MasterFixedAsset}" description="{FixedAssetDescription}"  />
</SelectDialog>
</core:FragmentDefinition>
