{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/user-api/currentUser$",
      "target": "/currentUser",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "^/sap/(.*)$",
      "target": "/sap/$1",
      "destination": "DS4_110_BAS",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/odata/v4/doa/(.*)$",
      "target": "/odata/v4/doa/$1",
      "destination": "SMU_DOA",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/odata/v4/attachment/(.*)$",
      "target": "/odata/v4/attachment/$1",
      "destination": "BTP_Sharepoint_Interface_Fin",
      "authenticationType": "xsuaa",
      "csrfProtection": false
    },
    {
      "source": "^/workflow/(.*)$",
      "target": "/workflow/$1",
      "destination": "spa_process_automation_workflow",
      "csrfProtection": false,
      "authenticationType": "none"
    },
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^/test-resources/(.*)$",
      "target": "/test-resources/$1",
      "authenticationType": "none",
      "destination": "ui5"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}